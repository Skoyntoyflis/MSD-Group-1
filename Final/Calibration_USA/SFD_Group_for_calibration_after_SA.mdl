{UTF-8}
Divorce rate=
	((1-W soc)*Education+W soc*Societal factor)
	~	
	~		|

Societal factor=
	1/(1 + EXP(-K*(Divorced/Population)))
	~	
	~	1/(1 + EXP(-K*(Divorced/Population)))
		
		DELAY1I(1/(1 + EXP(-K*(Divorced/Population))),Delay for societal,1/(1 + \
		EXP(-K*(Divorced/Population))))
		
		Divorced^N/(K^N+Divorced^N)
		
		IF THEN ELSE( Divorced/330>0.4, 0.58, 0.48)
	|

Scaled divorce=
	Divorce/Population*1000
	~	
	~		|

Marriage rate=
	0.3*(1-Societal factor)
	~	
	~		|

Divorce=
	Divorce rate*Married
	~	
	~	/Population*1000
	|

Population=
	Unmarried+Married+Divorced
	~	
	~		|

Delay for recovery=
	3
	~	 [0,20]
	~		|

Recovery=
	DELAY1( Divorced*Recovery rate , Delay for recovery)
	~	
	~		|

Divorced= INTEG (
	Divorce-Recovery,
		3.017e+07)
	~	
	~		|

Unmarried= INTEG (
	Recovery-Marriage,
		4.1795e+07)
	~	
	~		|

K=
	2.2
	~	 [-4,4,0.1]
	~		|

Marriage=
	Marriage rate*Unmarried
	~	
	~		|

Married= INTEG (
	Marriage-Divorce,
		1.13295e+08)
	~	
	~		|

Recovery rate=
	0.004
	~	 [0,1]
	~		|

Education=
	0.9
	~	 [0,1]
	~		|

W soc=
	0.338
	~	 [0,1]
	~		|

