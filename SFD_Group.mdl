Population=
	Unmarried+Married+Divorced
	~	
	~		|

Societal factor=
	1/(1 + EXP(-K*(Divorced/Population)))
	~	
	~	Divorced^N/(K^N+Divorced^N)
		
		IF THEN ELSE( Divorced/330>0.4, 0.58, 0.48)
	|

Delay for recovery=
	3
	~	
	~		|

Divorce=
	Divorce rate*Married
	~	
	~		|

Recovery=
	DELAY1( Divorced*Recovery rate , Delay for recovery)
	~	
	~		|

Divorced= INTEG (
	Divorce-Recovery,
		143)
	~	
	~		|

Economic factor=
	0.53333
	~	
	~		|

Unmarried= INTEG (
	Recovery-Marriage,
		47
		)
	~	
	~		|

K=
	1
	~	 [0,4,0.1]
	~		|

Marriage=
	Marriage rate*Unmarried
	~	
	~		|

Marriage rate=
	0.3-(Divorced/100000)
	~	
	~		|

Married= INTEG (
	Marriage-Divorce,
		140)
	~	
	~		|

Recovery rate=
	0.3
	~	
	~		|

Divorce rate=
	((1-W eco-W soc)*Education+W eco*Economic factor+W soc*Societal factor)/3
	~	
	~		|

Education=
	0.9
	~	
	~		|

W eco=
	0.33
	~	
	~		|

W soc=
	0.33
	~	
	~		|

