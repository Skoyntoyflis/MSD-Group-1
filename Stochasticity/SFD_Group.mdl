{UTF-8}
Economic factor=
	RANDOM UNIFORM(1-1/SQRT(TIME STEP),1+1/SQRT(TIME STEP) , shock seed ) * 0.53333
	~	 [0,1]
	~		|

Delay for societal=
	RANDOM UNIFORM(1-1/SQRT(TIME STEP),1+1/SQRT(TIME STEP) , shock seed ) * 0.466
	~	 [0,10]
	~		|

K=
	2.993
	~	 [-4,4,0.1]
	~		|

shock seed=
	1
	~	
	~		|

Scaled divorce=
	Divorce/Population*1000
	~	
	~		|

Marriage rate=
	0.3*(1-Societal factor)
	~	
	~		|

Divorce=
	Divorce rate*Married
	~	
	~	/Population*1000
	|

Societal factor=
	SMOOTH3( 1/(1 + EXP(-K*(Divorced/Population))), Delay for societal )
	~	
	~	1/(1 + EXP(-K*(Divorced/Population)))
		
		DELAY1I(1/(1 + EXP(-K*(Divorced/Population))),Delay for societal,1/(1 + \
		EXP(-K*(Divorced/Population))))
		
		Divorced^N/(K^N+Divorced^N)
		
		IF THEN ELSE( Divorced/330>0.4, 0.58, 0.48)
	|

Population=
	Unmarried+Married+Divorced
	~	
	~		|

Delay for recovery=
	3
	~	 [0,20]
	~		|

Recovery=
	DELAY1( Divorced*Recovery rate , Delay for recovery)
	~	
	~		|

Divorced= INTEG (
	Divorce-Recovery,
		3.017e+07)
	~	
	~		|

Unmarried= INTEG (
	Recovery-Marriage,
		4.1795e+07)
	~	
	~		|

Marriage=
	Marriage rate*Unmarried
	~	
	~		|

Married= INTEG (
	Marriage-Divorce,
		1.13295e+08)
	~	
	~		|

Recovery rate=
	0.002041
	~	 [0,1]
	~		|

Divorce rate=
	((1-W eco-W soc)*Education+W eco*Economic factor+W soc*Societal factor)
	~	
	~		|

Education=
	0.9
	~	 [0,1]
	~		|

W eco=
	1.134e-17
	~	 [0,1]
	~		|

W soc=
	0.3634
	~	 [0,1]
	~		|

